{
  "manifest_version": 3,
  "name": "Purchase Checker",
  "version": "1.0",
  "description": "Check your Robux spending on Developer Products and Game Passes.",
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "https://www.roblox.com/",
    "https://purchasechecker.me/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
